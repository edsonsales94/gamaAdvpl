Static Function BuscaSeq(_cAlias)
Local aArea		:= GetArea()
Local aAreaSX3	:= SX3->(GetArea())
Local cOrdem	:= "00"

SX3->(dbSetOrder(1))
SX3->(dbSeek(_cAlias))
do while SX3->( !eof() .and. X3_ARQUIVO =_cAlias )
	cOrdem := SX3->X3_ORDEM
	SX3->( dbSkip() )
enddo

RestArea(aAreaSX3)
RestArea(aArea)

Return(cOrdem)
