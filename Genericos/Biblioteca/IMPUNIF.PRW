Static Function ImpTabs(cTexto)
Local cDir 		:= "\data\data" + SM0->M0_CODIGO + "\"
Local aTabelas	:= Directory(cDir + "*.dbf")
Local nI, cNomeTab, cFilTab, cAlias, lFaz

oProcess:SetRegua2(Len(aTabelas))

SX2->(dbSetOrder(1))

cTexto  += "Tabelas: "
For nI := 1 to Len(aTabelas)

	oProcess:IncRegua2( "Append de informações..." )
    
    cAlias	 := Upper(Left(aTabelas[nI,1],3))
    cNameTab := Upper(Left(aTabelas[nI,1],6))
	cFilTab	 := iif(Left(cAlias, 1) == "S", Substr(cAlias, 2, 2), Left(cAlias, 3)) + "_FILIAL"
	lFaz	 := .F.
	
	if SM0->M0_CODIGO == "01"
		TcSqlExec("DELETE " + cNameTab + " WHERE " + cFilTab + " IN ('01','04')")
		lFaz := .T.
	else
		TcSqlExec("TRUNCATE TABLE " + cNameTab)
		lFaz := .T.
	endif
	
	if lFaz
		dbSelectArea(cAlias)
		Append From &(cDir + cNameTab)
	endif
	
	cTexto  += aTabelas[nI,1] + ", "
	
next
cTexto  += CRLF

Return()


