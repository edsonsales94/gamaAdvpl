Static Function ProcUni(lEnd)
Local _cFilterSx2 := ""
Local _aArea   := GetArea()     
Local _cForn 	:= ""
Local _cLoja	:= ""       
Local _cmsg, _natual, _ntotal
Local cOrc :=  left(mv_par01,15) //"ORCA2007       "
Local _cVersao := substr(mv_par01,16,4) // "0001"             


DbSelectArea("AK5")
DbSetOrder(1)          
DbSeek(xFilial("AK5")+mv_par02 /*"3" */ )
DbSelectArea("AK1")
DbGoTop()

_ntotal := AK5->(Reccount())

ProcRegua(_ntotal)

DbSelectArea("AK3")
DbSetOrder(1)                                                                    

_lAchPrd	:= .T.


_nAtual := 1
_lPula	:= .F.
While AK5->(!Eof()) .and. AK5->AK5_CODIGO <= mv_par03
	 IncProc(_nAtual)

		If AK3->(DbSeek(xfilial("AK3")+cOrc+_cVersao+AK5->AK5_COSUP) )
			cNivel	:= soma1(AK3->AK3_NIVEL)
		Else
			cNivel	:= "004"		
		Endif
		If Alltrim(AK5->AK5_MSBLQL) == "2" .OR. EMPTY(AK5->AK5_MSBLQL)
	  	 	RecLock("AK3",.T.)
	  	 	AK3->AK3_FILIAL 	:= xFilial("AK3")
	  	 	AK3->AK3_ORCAME 	:= cOrc
	  	 	AK3->AK3_VERSAO		:= _cVersao
	  	 	AK3->AK3_CO			:= AK5->AK5_CODIGO
	  	 	AK3->AK3_PAI		:= IIF(AK5->AK5_COSUP = ' ',cOrc,AK5->AK5_COSUP)
	  	 	AK3->AK3_TIPO		:= AK5->AK5_TIPO
	  	 	AK3->AK3_DESCRI		:= AK5->AK5_DESCRI
	  	 	AK3->AK3_NIVEL		:= cNivel
			_natual++      
			AK3->(MsUnlock())      
		Endif
		AK5->(DbSkip())
Enddo

//Alert("Processo Finalizado")
//Impr()
Return NIL




