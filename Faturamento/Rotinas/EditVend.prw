User Function ValVend      

Local lret := .T.     

	if(M->C5_CGC $ "00776574000741")  
		If ( M->C5_VEND1 $ "000043/3321  " ) 
			lret := .T. 
		else
			lret := .F.
		endif	
	elseif ( POSICIONE("SA1",1,xFilial("SA1")+M->C5_CLIENTE+M->C5_LOJACLI, "A1_XCOMPAR") == "1" )
		if ( M->C5_VEND1 == POSICIONE("SA1",1,xFilial("SA1")+M->C5_CLIENTE+M->C5_LOJACLI, "A1_VEND") .or. ;
			 M->C5_VEND1 == POSICIONE("SA1",1,xFilial("SA1")+M->C5_CLIENTE+M->C5_LOJACLI, "A1_XVENALT") ) 
			lret := .T. 
		else
			lret := .F.
		endif
	else
		lret := .T.
	endif                                      

Return lret

