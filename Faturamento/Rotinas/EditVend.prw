User Function EditVend      

Local lret := .F.

If M->C5_TIPO $ "B/D"
	lret := .T.	
Elseif M->C5_CGC $ "00776574000741"
	lret := .T.	                                      
Elseif (POSICIONE("SA1",1,xFilial("SA1")+M->C5_CLIENTE+M->C5_LOJACLI, "A1_XCOMPAR") == "1")
	lret := .T.
EndIf     

Return lret      


